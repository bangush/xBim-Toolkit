<Window x:Class="XbimXplorer.FederatedModelDlg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xbim="http://schemas.Xbim.com/Presentation"
        Title="FederatedModelDlg" Height="300" Width="300">
    <Grid  >
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" MinHeight="23" />
        </Grid.RowDefinitions>
        <xbim:FederatedModelProperties Grid.Row="0" Margin="0,5,0,15"></xbim:FederatedModelProperties>
        <Button x:Name="Add" Grid.Row="1" Margin="20,0,20,0" Click="Add_Click">Add Model</Button>
    </Grid>
</Window>
