<Window x:Class="XbimXplorer.PropertiesWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:xbim="http://schemas.Xbim.com/Presentation"
    xmlns:local="clr-namespace:XbimXplorer"
    Title="Properties" Height="300" Width="300"
        >
    <Grid>
        <xbim:IfcMetaDataControl>
            <xbim:IfcMetaDataControl.IfcInstance>
                <Binding Path="Instance">
                    <Binding.RelativeSource>
                        <RelativeSource
                            Mode="FindAncestor"
                            AncestorType="{x:Type local:PropertiesWindow}"
                            AncestorLevel="1"
                            />
                    </Binding.RelativeSource>
                   
                </Binding>

            </xbim:IfcMetaDataControl.IfcInstance>
        </xbim:IfcMetaDataControl>
    </Grid>
</Window>
