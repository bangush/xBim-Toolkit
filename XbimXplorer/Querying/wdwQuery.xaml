<Window x:Class="XbimXplorer.Querying.wdwQuery"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Model Querying" Height="381.6" Width="603.2">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="150"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox x:Name="txtCommand" Grid.Row="0" FontFamily="Lucida Console" KeyDown="txtCommand_KeyDown" AcceptsReturn="True" AutoWordSelection="True" />
        <TextBox x:Name="txtOut" Grid.Row="1" FontFamily="Lucida Console" AcceptsReturn="True" AutoWordSelection="True" />
        <GridSplitter Height="5"  Grid.Row="0" VerticalAlignment="Bottom" HorizontalAlignment="Stretch" />
    </Grid>
</Window>
